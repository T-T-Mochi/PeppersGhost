import cv2

fps = 60
size = 64

fourcc = cv2.VideoWriter_fourcc('m','p','4','v')
video = cv2.VideoWriter('3D.mp4', fourcc, fps, (size, size))

for i in range(1, 3601):
    img = cv2.imread('frames/'+str(i)+'.png')
    img2 = cv2.resize(img, dsize=(size,size))
    print(i)
    video.write(img2)

video.release()
